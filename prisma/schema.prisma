datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model CryptoData {
  id           Int      @id @default(autoincrement())
  rate         Float
  executed     Float
  low          Float
  high         Float
  comByCoin    Float
  comSellUSDT  Float
  active       Float
  coin         String
  status       String
  purchaseDate DateTime
  saleDate     DateTime
  orderDate    DateTime
}
